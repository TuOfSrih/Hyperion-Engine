<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <VulkanSDK>C:\VulkanSDK\1.1.121.2</VulkanSDK>
    <glmDir>$(VulkanSDK)\Third-Party\Include</glmDir>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>Custom</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="VulkanSDK">
      <Value>$(VulkanSDK)</Value>
    </BuildMacro>
    <BuildMacro Include="glmDir">
      <Value>$(glmDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>